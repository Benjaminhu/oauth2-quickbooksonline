{
    "name": "liftkeeper/oauth2-quickbooksonline",
    "description": "QuickBooksOnline OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "type": "library",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "identity",
        "sso",
        "single sign on"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Simon"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "1.*|2.*"
    },
    "autoload": {
        "psr-4": {
            "Liftkeeper\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Liftkeeper\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
